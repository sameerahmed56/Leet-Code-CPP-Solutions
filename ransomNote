class Solution {
public:
    bool canConstruct(string ransomNote, string magazine) {
        int n = ransomNote.size(), m = magazine.size();
        bool hasChar = false; 
        for(int i = 0; i < n; i++){
            hasChar = false;
            for(int j = 0; j < m; j++){
                if(ransomNote[i] == magazine[j]){
                    magazine[j] = 'A';
                    hasChar = true;
                    break;
                }
            }
            if(!hasChar){
                return false;
            }
        }
        return true;
    }
};
